  "dependencies": {
    "dotenv": "^16.0.3",
    "helmet": "^7.0.0",
    "jose": "^4.14.4",
    "koa-body": "^6.0.1",
    "koa-ejs": "^4.3.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^12.0.0",
    "lodash.isempty": "^4.4.0",
    "lodash.snakecase": "^4.1.1",
    "mongodb": "^5.5.0",
    "nodemon": "^2.0.22",
    "npm-check-updates": "^16.10.12",
    "oidc-provider": "^8.2.1",
    "pg": "^8.11.0",
    "sequelize": "^6.31.1",
    "sequelize-cli": "^6.6.0",
    "uuid": "^9.0.0"
